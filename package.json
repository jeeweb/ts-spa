    "express": "^4.18.2",
    "nodemon": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "fix-esm-import-path": "^1.5.0",
    "install": "^0.13.0",
    "npm": "^10.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
